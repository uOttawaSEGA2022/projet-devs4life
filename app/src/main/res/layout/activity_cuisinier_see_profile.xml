<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tv_"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CuisinierSeeProfileActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_profile"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="This is your profile"
            android:fontFamily="@font/k2d_medium"
            android:textColor="@color/textBlue"
            android:textSize="40sp"/>

        <TextView
            android:id="@+id/tv_profileName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Name"
            android:fontFamily="@font/k2d_medium"
            android:textColor="@color/textBlue"
            android:textSize="20sp"/>/>

        <TextView
            android:id="@+id/tv_profileEmail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Email"
            android:fontFamily="@font/k2d_medium"
            android:textColor="@color/textBlue"
            android:textSize="20sp"/>/>

        <TextView
            android:id="@+id/tv_profileRating"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Rating"
            android:fontFamily="@font/k2d_medium"
            android:textColor="@color/textBlue"
            android:textSize="20sp"/>/>

        <TextView
            android:id="@+id/tv_profileDescription"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Description"
            android:fontFamily="@font/k2d_medium"
            android:textColor="@color/textBlue"
            android:textSize="20sp"/>/>

        <TextView
            android:id="@+id/tv_profileAddress"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Address"
            android:fontFamily="@font/k2d_medium"
            android:textColor="@color/textBlue"
            android:textSize="20sp"/>/>

        <Button
            android:id="@+id/btn_goBackProfile"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Go Back"
            android:background="@drawable/rounded_btn"
            android:fontFamily="@font/k2d_medium"
            android:textColor="@color/textBlue"
            android:textSize="20sp"/>
            />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>